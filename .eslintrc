{
  "ecmaVersion": 6,
  "ecmaFeatures": {
    "jsx": false,
    "modules": false
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "qunit": true
  },
  "rules": {
    "quotes": [2, "single"],
    "strict": "off"
  },
  "parserOptions": {
      "sourceType": "module"
  },
  "plugins": []
}
